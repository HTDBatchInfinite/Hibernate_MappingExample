<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.infinite.hibernate.pojo.Bord" table="Bord_table">
		<id name="bordId" column="Bord_Id" type="int" length="20">
			<generator class="increment"></generator>
		</id>
		<property name="bordName" column="Bord_name"/>
	</class>
</hibernate-mapping>